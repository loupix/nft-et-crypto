{
  "name": "client",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "clean": "yarn rimraf ./dist",
    "translate": "ng extract-i18n --output-path src/locale --format=json",
    "start": "ng serve --verbose",
    "start:en": "ng serve --configuration=en",
    "start:fr": "ng serve --configuration=fr",
    "watch": "ng build --watch --configuration development",
    "watch:en": "ng serve --configuration=en --live-reload --watch --open",
    "watch:fr": "ng serve --configuration=fr --live-reload --watch --open",
    "build": "yarn clean && ng build",
    "build:en": "yarn clean && ng build --configuration=en",
    "build:fr": "yarn clean && ng build --configuration=fr",
    "test": "ng test"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.3.0",
    "@angular/cdk": "^13.3.4",
    "@angular/common": "~13.3.0",
    "@angular/core": "~13.3.0",
    "@angular/flex-layout": "^13.0.0-beta.38",
    "@angular/forms": "~13.3.0",
    "@angular/material": "^13.3.4",
    "@angular/platform-browser": "~13.3.0",
    "@angular/platform-browser-dynamic": "~13.3.0",
    "@angular/router": "~13.3.0",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.3.3",
    "@angular-devkit/schematics": "^13.3.3",
    "@angular/cli": "~13.3.3",
    "@angular/compiler": "^13.3.4",
    "@angular/compiler-cli": "~13.3.0",
    "@angular/localize": "^13.3.4",
    "@types/bcryptjs": "^2.4.2",
    "@types/jasmine": "~3.10.0",
    "@types/jest": "^27.4.1",
    "@types/lodash": "^4.14.182",
    "@types/mocha": "^9.1.1",
    "@types/node": "^12.11.1",
    "@types/webpack": "^5.28.0",
    "jasmine": "^4.1.0",
    "jasmine-core": "~4.0.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "rimraf": "^3.0.2",
    "typescript": "~4.6.2"
  }
}
